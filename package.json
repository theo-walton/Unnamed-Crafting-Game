{
  "name": "electron-react-bun-ts",
  "version": "0.0.1",
  "private": true,
  "main": "src/main/main.js",
  "scripts": {
    "dev:renderer": "vite",
    "build:renderer": "vite build",
    "start": "electron .",
    "build:app": "npm run build:renderer && electron-builder --win --x64"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.0.0",
    "electron": "^39.2.7",
    "electron-builder": "^24.6.0",
    "electron-packager": "^17.1.0",
    "typescript": "^5.0.0",
    "vite": "^6.4.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "build": {
    "appId": "com.example.electronreactbun",
    "files": [
      "dist/renderer/**/*",
      "src/main/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis"
      ]
    }
  }
}
